unit Unit_10EasyPieces_Funtions;

interface

implementation


function fibonacci(initial_value, lengh : integer):String;
var
 row, sum: Integer;
 line: string;

begin
row := 0;
  while lengh < row do begin
   sum := initial_value + (initial_value + 1);
   initial_value := sum;
   line := line + sum + ', '
   row := row + 1;
  end;
  Result := line;

end;

end.
